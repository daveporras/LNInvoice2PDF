{
  "expo": {
    "name": "LN Invoice Reader",
    "description": "Mobile App for read LN Invoices and do many things!",
    "slug": "LNInvoiceReader",
    "owner": "tiendasatoshi",
    "version": "0.1",
    "githubUrl": "https://github.com/satoshidave/LNInvoiceReader",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#292929",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#292929"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#292929"
      },
      "package": "com.tiendasatoshi.LNInvoiceReader"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
